{
  "family": "webhookbot",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "webhookbot",
      "image": "167737505636.dkr.ecr.us-east-1.amazonaws.com/webhookbot:latest",
      "cpu": 256,
      "memory": 1024,
      "memoryReservation": 256,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"]
}
